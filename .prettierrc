{
  "semi": true,
  "trailingComma": "all",
  "singleQuote": true,
  "printWidth": 120,
  "tabWidth": 2,
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true,
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(react)",
    "^(react-dom)",
    "^(react-redux)",
    "^\\w",
    "^@mockplus/(.*)$",
    "^(@)",
    "^(^./(.*).s?css)$",
    "^[../*]"
  ]
}
